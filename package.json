{
    "name": "one-trick-electron",
    "version": "1.0.0",
    "description": "",
    "main": "dist/main.js",
    "targets": {
        "main": {
            "source": "src/main/main.ts",
            "context": "electron-main"
        },
        "renderer": {
            "source": "src/renderer/index.html",
            "distDir": "dist/renderer/",
            "publicUrl": "./"
