        "dev": "parcel watch --no-cache",
        "build": "parcel build"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "parcel": "^2.8.3",
        "parcel-reporter-static-files-copy": "^1.5.0",
        "process": "^0.11.10"
    },
    "dependencies": {
        "electron": "^23.1.3",
        "node-fetch-commonjs": "^3.2.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "ws": "^8.13.0"
    }
